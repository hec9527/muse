{
  "name": "muse",
  "displayName": "Muse",
  "description": "muse is publish tool for bid",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:muse.start",
    "workspaceContains:src/p"
  ],
  "author": "hec9527",
  "publisher": "hec9527",
  "license": "MIT",
  "icon": "logo.png",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "http://gitlab.100credit.cn/fed/muse.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "muse.start",
        "title": "start",
        "category": "muse"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+p ctrl+p",
        "mac": "ctrl+p ctrl+p",
        "command": "muse.start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "vsix": "vsce package -o muse.vsix && code --install-extension muse.vsix",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vsce": "^1.85.0",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
