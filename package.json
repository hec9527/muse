{
  "name": "muse",
  "displayName": "Muse",
  "description": "muse is publish tool for bid",
  "version": "0.2.5",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:muse.webview",
    "workspaceContains:src/p"
  ],
  "author": "hec9527",
  "publisher": "hec9527",
  "license": "MIT",
  "icon": "logo.png",
  "main": "./dist/src/extension.js",
  "repository": {
    "type": "git",
    "url": "http://gitlab.100credit.cn/fed/muse.git"
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "yarn run clear && tsc -p ./",
    "vsix": "vsce package -o muse.vsix && code --install-extension muse.vsix",
    "lint": "eslint src --ext ts",
    "pub": "yarn lint && vsce publish",
    "clear": "rm -rf dist",
    "dev": "npm run dev:webview & npm run dev:extension",
    "dev:webview": "webpack -w",
    "dev:extension": "tsc -w --module commonjs -p ./",
    "build": "npm run build:webview & npm run build:extension",
    "build:webview": "webpack --mode production",
    "build:extension": "tsc --module commonjs -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "muse.webview",
        "title": "webview",
        "category": "Muse"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+p ctrl+p",
        "mac": "ctrl+p ctrl+p",
        "command": "muse.webview"
      }
    ],
    "configuration": {
      "title": "Muse",
      "properties": {
        "muse.autoOpenLog": {
          "type": "boolean",
          "default": false,
          "description": "发布成功后自动在浏览器中打开日志"
        },
        "muse.hideDisabledFilter": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "### 隐藏被禁用的过滤器\n页面选择的过滤器是按照页面首字母编排的，如果某个首字母的页面不存在，该字母将会被禁用，隐藏没用的过滤器可以减少不必要内容，在小窗模式或者放大模式下不至于超出屏幕边界"
        }
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.24.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "mocha": "^8.1.3",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "vsce": "1.93.0",
    "vscode-test": "1.5.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
  }
}
